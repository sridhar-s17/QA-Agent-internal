    "close_button": "svg[data-icon-testid='maximizer_close_icon']"
  },
  "design_document": {
    "document_tile": "div.tile",
    "document_card": "div[data-clickable-testid='chat-message-Discovery Document-card']",
    "close_button": "svg[data-icon-testid='maximizer_close_icon']"
  },
  "test_document": {
    "test_card": "div[data-clickable-testid='chat-message-Test Document-card']",
    "close_button": "svg[data-icon-testid='maximizer_close_icon']"
  },
  "qr_code": {
    "qr_container": "div[data-context-testid='chat_message_multiqr']"
  },
  "error_handling": {
    "retry_button": "label[for*='retry']",
    "retry_radio": "input[id*='retry']"
