{
  "login": {
    "username_input": "input[data-input-testid='login_email']",
    "password_input": "input[data-input-testid='login_password']",
    "login_button": "button[data-clickable-testid='login_button']"
  },
  "studio": {
    "default_prompt": "div[data-clickable-testid='quick_start_card_0']"
  },
  "questions": {
    "radio_option": "[data-clickable-testid='current_radio_option_0']",
    "checkbox_option": "[data-clickable-testid='current_checkbox_option_0']",
    "selection_done_button": "[data-clickable-testid='checkbox_options_next_button']",
    "selection_done_button_css": "button[style*='background: rgb(3, 98, 255)']",
    "selection_done_button_xpath": "//button[contains(text(), 'Selection Done')]"
  },
  "document_flow": {
    "decision_input": "textarea[data-input-testid='chat_prompt_input']",
